{
  "name": "sphere",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "deno task dev:api & deno task dev:vite",
    "test": "deno task test:api",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:api": "deno run --allow-env --allow-net --allow-read --allow-sys api/src/backend.ts",
    "dev:vite": "deno run -A npm:vite",
    "test:api": "deno test --allow-env --allow-net --allow-read --allow-sys api/tests/",
    "deploy:vercel": "sh scripts/deploy-vercel.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
